url = {https://www.sciencedirect.com/science/article/pii/S089982560500059X},
author = {Axel Ockenfels and Alvin E. Roth},
keywords = {Internet auctions, Second price auctions, eBay, Amazon, Sniping, Market design},
abstract = {In second price Internet auctions with a fixed end time, such as those on eBay, many bidders submit their bids in the closing minutes or seconds of an auction. We propose an internet auction model, in which very late bids have a positive probability of not being successfully submitted, and show that late bidding in a fixed deadline auction can occur at equilibrium in auctions both with private values and with uncertain, dependent values. Late bidding may also arise out of equilibrium, as a best reply to incremental bidding. However, the strategic advantages of late bidding are severely attenuated in auctions that apply an automatic extension rule such as auctions conducted on Amazon. Field data show that there is more late bidding on eBay than on Amazon, and this difference grows with experience. We also study the incidence of multiple bidding, and its relation to late bidding.}
}
@article{NYSTROM2016119,
