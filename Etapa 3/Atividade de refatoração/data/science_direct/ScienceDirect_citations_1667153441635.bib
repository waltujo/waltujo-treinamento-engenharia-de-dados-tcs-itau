@article{AHMADI201460,
title = {Learning solutions for auction-based dynamic spectrum access in multicarrier systems},
journal = {Computer Networks},
volume = {67},
pages = {60-73},
year = {2014},
issn = {1389-1286},
doi = {https://doi.org/10.1016/j.comnet.2014.03.026},
