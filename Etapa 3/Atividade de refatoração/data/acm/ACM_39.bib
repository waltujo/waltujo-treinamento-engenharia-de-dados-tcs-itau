@inproceedings{10.1145/3221269.3222331,title = {PathGraph: querying and exploring big data graphs}, author = {Colazzo Dario , Mecca Vincenzo , Nol\u00e9 Maurizio , Sartiani Carlo },year = {2018}, isbn = {9781450365055}, publisher = {Association for Computing Machinery}, address = {New York, NY, USA}, url = {https://doi.org/10.1145/3221269.3222331}, doi = {10.1145/3221269.3222331}, abstract = {With the widespread diffusion of social networks and the dawn of data-intensive scientific applications, graphs became one of the foundations for modern data management applications. A key role in graph querying and analysis is played by Regular Path Queries, their extensions, and, in particular, GXPath. In this demo we will present PathGraph, a distributed GXPath query processor, and its web-based graphical interface.}, location = {Bozen-Bolzano, Italy}, series = {SSDBM '18}, pages = {1\u20134}, numpages = {4}, keywords = {big data, RPQs, graphs}}
@inproceedings{10.1145/3482632.3483051,title = {Exploration of the Scientific Training Path of Ethnic Dance Based on Big Data Analysis}, author = {Tan Xi },year = {2021}, isbn = {9781450390255}, publisher = {Association for Computing Machinery}, address = {New York, NY, USA}, url = {https://doi.org/10.1145/3482632.3483051}, doi = {10.1145/3482632.3483051}, abstract = {In today's digitization and informationization, all walks of life in society are labeled as BD. With the abundance of data information and increased data openness, various fields have been affected by BD. With the rapid growth of the Internet and new media, ethnic dance has inevitably entered the \"BD era\" as a manifestation of traditional culture. Way, folk dance has always played an important role, and its unique performance and ethnic style stimulate people's senses to give it artistic charm. Due to the continuous acceleration of the overall reform process in our country, national dances are also faced with the task of how to advance with the times, and scientific training is a breakthrough in this issue. This article aims to study the application of BD research methods in the field of national dance scientific training. This article uses case study methods, literature research methods, survey methods, mathematical statistical analysis methods, etc. to explore how BD can provide scientific training methods for ethnic dance, and combine the research results of previous experts or scholars to further explore ethnic dance under BD thinking. The scientific training path forms a relatively comprehensive understanding. Then through the sorting and analysis of various national dance technique materials, combining with practice and theory, put forward the content of scientific training, and then summarize the solutions to the difficulties faced by national dance training under the BD thinking. Experiments show that there are not many folk dance educators who know BD well, only 27%, but most teachers and students still have a certain understanding of BD, and believe that BD can make folk dance training methods more scientific or It has an auxiliary effect on the training methods of national dance.}, location = {Dalian, China}, series = {ICISCAE 2021}, pages = {920\u2013923}, numpages = {4}}
