@inproceedings{10.1145/3495018.3495503,title = {Research on the Application of Big Data Information Processing Technology in the Improvement of English Language Fossilization}, author = {Li Jingtai },year = {2021}, isbn = {9781450385046}, publisher = {Association for Computing Machinery}, address = {New York, NY, USA}, url = {https://doi.org/10.1145/3495018.3495503}, doi = {10.1145/3495018.3495503}, abstract = {Many students generally face a problem In the acquisition stage of language and culture. Some language errors are still unavoidable after repeated corrections, and a solidified error mode is gradually formed, that is, language \"fossilization\". This article attempts to adopt a teaching mode combining big data to cultivate students' cognitive flexibility and construct contextual knowledge with foreign language features, so as to alleviate fossilization and enable learners to master the real target language.}, location = {Manchester, United Kingdom}, series = {AIAM2021}, pages = {1865\u20131867}, numpages = {3}}
@inproceedings{10.1145/3482632.3483137,title = {Research on Middle School Physics Teaching Reform Based on Flipped Classroom in the Era of Big Data}, author = {Yu Ping , Yang Nengxun },year = {2021}, isbn = {9781450390255}, publisher = {Association for Computing Machinery}, address = {New York, NY, USA}, url = {https://doi.org/10.1145/3482632.3483137}, doi = {10.1145/3482632.3483137}, abstract = {With the rapid development of the era of big data, the continuous updating of high technology such as the Internet and 5G, the increase in demand for talents by the people, and the importance of people on education have gradually emerged. With the support of the era of big data, education enables students to gain more interesting, useful and exciting content in addition to book knowledge. Among them, \"flipped classroom\", an innovative teaching model that combines computer networks and teaching, has to be amazing. Judging from the schools that have tried this teaching model, the positive effect of this model is still very obvious. Not only does it increase the enthusiasm of students in class, gain more knowledge and apply the learned knowledge to life according to the situation, but also allows more interaction between teachers and students throughout the teaching process, and the teaching effect is more significant. This article takes the middle school physics teaching as the object, analyzes the problems in the teaching process, and uses the contrast method to compare the traditional teaching mode with the flipped classroom teaching mode. Analyze the characteristics of the latter in the teaching process, the problems in the operation process, and the feasibility of using this mode in middle school physics. It is hoped that through the analysis and research of the flipped classroom teaching model, the school can strengthen the emphasis on this model in the physics teaching process. And have a deeper understanding of this model to promote its further development.}, location = {Dalian, China}, series = {ICISCAE 2021}, pages = {1298\u20131301}, numpages = {4}}
@inproceedings{10.1145/3469213.3470322,title = {Research on the Construction of Enterprise E-commerce Cloud Platform Based on Big Data}, author = {Yang Ming },year = {2021}, isbn = {9781450390200}, publisher = {Association for Computing Machinery}, address = {New York, NY, USA}, url = {https://doi.org/10.1145/3469213.3470322}, doi = {10.1145/3469213.3470322}, location = {Chongqing, China}, series = {ICAIIS 2021}, pages = {1\u20135}, numpages = {5}}
@inproceedings{10.1145/3418094.3418130,title = {A Cloud Based Big Data Health-Analytics-as-a-Service Framework to Support Low Resource Setting Neonatal Intensive Care Unit}, author = {Bastwadkar Meghana , McGregor Carolyn , Balaji S. },year = {2020}, isbn = {9781450377768}, publisher = {Association for Computing Machinery}, address = {New York, NY, USA}, url = {https://doi.org/10.1145/3418094.3418130}, doi = {10.1145/3418094.3418130}, abstract = {Critical care patients are monitored by a range of medical devices collecting high frequency data. New computing frameworks and platforms are being proposed to review and analyze the data in detail. The application of these approaches in a low resource setting is challenged by the approaches used for data acquisition. Software as a Service (SaaS) is a form of cloud computing where a cloud-based software application enables the storage, analysis and visualization of data within the cloud. A subset of SaaS is Health Analytics as a Service (HAaaS), which provides software to support health analytics in the cloud. The objective of this study is to design, implement, and demonstrate an extendable big-data compatible HAaaS framework that offers both real-time and retrospective analysis where data acquisition is not tightly coupled. A data warehousing framework is presented to facilitate analysis within a low resource setting. The framework has been instantiated in the Artemis platform within the context of the Belgaum Children Hospital (BCH) case study. Initial end-to-end testing with the Nellcor monitor (bedside monitor at BCH), which was not connected to any human, was completed. This testing confirms the functionality of the new Artemis cloud instance to receive data from test device using an alternate data acquisition approach.}, location = {Kamakura City, Japan}, series = {ICMHI '20}, pages = {30\u201336}, numpages = {7}, keywords = {big data, health informatics, retrospective analysis, cloud computing, real-time analytics, analytics-as-a-service, premature infants, decision support system, physiological data}}
@inproceedings{10.1145/2693208.2693228,title = {Modern Enterprises in the Bubble: Why Big Data Matters}, author = {Caldarola Enrico Giacinto , Picariello Antonio , Castelluccia Daniela },year = {2015}, publisher = {Association for Computing Machinery}, address = {New York, NY, USA}, url = {https://doi.org/10.1145/2693208.2693228}, doi = {10.1145/2693208.2693228}, abstract = {In the past few years, a massive amount of data has been delivered by an increased and ubiquitous use of Information and Communication Technologies (ICTs) in human activities and a propagation of smart devices or smart sensors, which continuously connect people and things in cyberspace. This huge bubble of data is a gold mine: it is an unlimited source of knowledge and insights about habits and preferences of people and has captured the attention of modern enterprises. Companies look at this data with interest and purpose to gain competitive advantage by applying analytics tools over them. In this context, a new approach is required for mastering data without the risk of ending up in the bubble and collecting a huge meaningless pile of junk data. Starting from a definition of new approaches, this work outlines Big Data strategies for modern enterprises and highlights challenges, emergent solutions and open issues.}, pages = {1\u20134}, numpages = {4}, keywords = {Knowledge management, Enterprise 2.0, data integration, data analytics, Big Data, data storage, Business Intelligence}}
